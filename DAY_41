# 3005. Count Elements With Maximum Frequency 

class Solution {
public:
    int maxFrequencyElements(vector<int>& nums) {
        vector<int> freq(101, 0);
        for (int num : nums) {
            freq[num]++;
        }

        int maxFreq = 0;
        for (int f : freq) {
            maxFreq = max(maxFreq, f);
        }

        int result = 0;
        for (int f : freq) {
            if (f == maxFreq) result += f;
        }

        return result;
    }
};
