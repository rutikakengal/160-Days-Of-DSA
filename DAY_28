# 2749. Minimum Operations to Make the Integer Zero

class Solution {
public:
    int makeTheIntegerZero(int num1, int num2) {
        for (int k = 1; k <= 60; k++) {
            long long remain = (long long)num1 - (long long)k * num2;
            if (remain < k) continue; 
            if (__builtin_popcountll(remain) <= k) return k;
        }
        return -1;
    }
};
